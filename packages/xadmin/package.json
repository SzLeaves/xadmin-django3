{
  "name": "xadmin",
  "version": "3.0.1",
  "description": "Xadmin Core",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/xadmin.js",
  "author": "sshwsfc@gmail.com",
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.2"
  },
  "peerDependencies": {
    "lodash": "4",
    "react": ">=16.8",
    "react-dom": ">=16.8",
    "react-redux": "8",
    "react-router": "6",
    "react-router-dom": "6",
    "redux": "4",
    "redux-saga": "1"
  },
  "devDependencies": {},
  "scripts": {
    "build:lib": "babel src --root-mode upward --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --root-mode upward --out-dir es",
    "build:umd": "rollup -c ../../rollup.config.js",
    "clean": "rm -rf lib es dist",
    "build": "npm run clean && npm run build:lib && npm run build:es && npm run build:umd"
  },
  "files": [
    "lib",
    "es",
    "dist"
  ]
}
